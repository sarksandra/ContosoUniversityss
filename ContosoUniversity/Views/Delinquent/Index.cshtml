@model IEnumerable<ContosoUniversity.Models.Delinquent>

@{
    ViewData["Title"] = "Index";

 }

<h1>Delinquent Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(Model => Model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.FirstMidName)
            </th>
            
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelitem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.FirstMidName)
                </td>
                <td colspan="0">
                    <a asp-action="Edit" class="btn btn-secondary" asp-route-id="@item.ID">Edit</a>
                    <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.ID">Details</a>
                    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.ID">Delete</a>
                    <a asp-action="Clone" class="btn btn-danger" asp-route-id="@item.ID">Clone</a>
                    

                </td>
            </tr>
        }
    </tbody>

</table>
